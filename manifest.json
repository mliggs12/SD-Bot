{
  "manifest_version": 3,
  "name": "SD Bot",
  "version": "1.2.1",
  "description": "Bot for automating various service desk functions",
  "icons": {
    "128": "images/icon-128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://max.niceincontact.com/*",
    "https://support.houseloan.com/*"
  ]
}