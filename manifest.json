{
  "manifest_version": 3,
  "name": "SD Bot",
  "version": "1.3.2",
  "description": "Bot for automating various service desk functions",
  "icons": {
    "128": "dist/images/icon-128.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "sidePanel",
    "storage",
    "tabs"
  ],
  "action": {},
  "background": {
    "service_worker": "dist/background.ts"
  },
  "side_panel": {
    "default_path": "dist/sidepanel/sidepanel.html"
  },
  "host_permissions": [
    "https://max.niceincontact.com/*",
    "https://support.houseloan.com/*"
  ]
}